import spacy
from spacy import displacy

nlp = spacy.load("en_core_web_sm")

multiline_text = """
Artificial Intelligence (AI) has become an essential part of modern technology, influencing various industries 
such as healthcare, finance, education, and transportation. 
By using complex algorithms and large datasets, AI systems can learn patterns, make predictions, 
and even perform tasks that once required human intelligence. 
However, ethical concerns and data privacy issues continue to challenge the widespread adoption of AI worldwide.
"""

multiline_doc = nlp(multiline_text)

for token in multiline_doc:
    print(
        f"""
TOKEN: {token.text}
=====
{token.tag_ = }
{token.head.text = }
{token.dep_ = }"""
    )

displacy.serve(multiline_doc, style="dep")
